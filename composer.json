{
    "name": "symplify/symplify-kernel",
    "description": "Internal Kernel for Symplify packages",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symplify/smart-file-system": "^10.3",
        "symplify/composer-json-manipulator": "^10.3",
        "symplify/autowire-array-parameter": "^10.3",
        "symplify/package-builder": "^10.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymplifyKernel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.3-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<10.2.0",
        "symplify/easy-coding-standard": "<10.2.0",
        "symplify/phpstan-rules": "<10.2.0",
        "symplify/easy-testing": "<10.2.0",
        "symplify/rule-doc-generator-contracts": "<10.2.0",
        "symplify/php-config-printer": "<10.2.0",
        "symplify/amnesia": "<10.2.0",
        "symplify/phpstan-extensions": "<10.2.0",
        "symplify/rule-doc-generator": "<10.2.0",
        "symplify/vendor-patches": "<10.2.0",
        "symplify/skipper": "<10.2.0",
        "symplify/symfony-static-dumper": "<10.2.0",
        "symplify/git-wrapper": "<10.2.0",
        "symplify/monorepo-builder": "<10.2.0",
        "symplify/config-transformer": "<10.2.0",
        "symplify/easy-ci": "<10.2.0",
        "symplify/coding-standard": "<10.2.0",
        "symplify/latte-phpstan-compiler": "<10.2.0",
        "symplify/template-phpstan-compiler": "<10.2.0",
        "symplify/phpstan-latte-rules": "<10.2.0",
        "symplify/easy-parallel": "<10.2.0",
        "symplify/neon-config-dumper": "<10.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
